<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>合肥市书画院</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <script src="../../Areas/Admin/Scripts/ueditor/ueditor.all.js" type="text/javascript"></script>
    <script src="../../Areas/Admin/Scripts/ueditor/ueditor.all.min.js" type="text/javascript"></script>
    <script src="../../Areas/Admin/Scripts/ueditor/ueditor.config.js" type="text/javascript"></script>
    <link href="../../Areas/Admin/Scripts/ueditor/themes/iframe.css" rel="stylesheet" type="text/css" />
    
    @Styles.Render("~/Content/css")
    @*@Styles.Render("~/Content/Site.css")*@
    @Styles.Render("~/Content/yc_site.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts/jquery.js")
    @Scripts.Render("~/Scripts/bootstrap.js")
</head>
<body>
    <div class="yc_global_header">
        <a href="#">书画院微博</a> | <a href="/Logion/">后台登陆</a>
    </div>
    <div class="YC_global_nav">
        <div class="yc_bg">
            <div class="yc_header">
                <div class="yc_nav">
                    <div class="yc_title current">
                        <a href="/Home/Index" class="yc_tab first_tab"><i>首页</i></a>
                    </div>
                    <div class="yc_title current">
                        <a href="/Wed/Index" class="yc_tab" id="yc_select_gaikuang" onmouseover="mopen('yc_slide_my')" onmouseout="mclosetime()"><i>概况</i></a>
                        <div id="yc_slide_gaikuang" onmouseover="mcancelclosetime()" onmouseout="mclosetime()" style="z-index:1000">
                            @Html.ActionLink("高端婚庆策划","Index", "Wed")
                            @Html.ActionLink("高端商务礼仪","Index", "Business")
                            @Html.ActionLink("高端汽车服务","Index", "CarService")
                            @Html.ActionLink("车辆订购、按揭","Index", "CarOrder")
                            @Html.ActionLink("车辆抵押","Index", "Pledge")
                            @Html.ActionLink("二手车交易、评估","Index", "HandCar")
                        </div>
                    </div>
                    <div class="yc_title current">
                        <a href="/News/Index" class="yc_tab"><i>新闻动态</i></a>
                    </div>
                    <div class="yc_title current">
                        <a href="/Offer/Index" class="yc_tab"><i>优惠活动</i></a>
                    </div>
                    <div class="yc_title current">
                        <a href="/Wed/Index" class="yc_tab" id="yc_select" onmouseover="mopen('yc_slide_my')" onmouseout="mclosetime()"><i>服务范围</i></a>
                        <div id="yc_slide_my" onmouseover="mcancelclosetime()" onmouseout="mclosetime()" style="z-index:1000">
                            @Html.ActionLink("高端婚庆策划","Index", "Wed")
                            @Html.ActionLink("高端商务礼仪","Index", "Business")
                            @Html.ActionLink("高端汽车服务","Index", "CarService")
                            @Html.ActionLink("车辆订购、按揭","Index", "CarOrder")
                            @Html.ActionLink("车辆抵押","Index", "Pledge")
                            @Html.ActionLink("二手车交易、评估","Index", "HandCar")
                        </div>
                    </div>
                    <div class="yc_title current">
                        <a href="/AboutUs/Index" class="yc_tab"><i>关于我们</i></a>
                    </div>
                    <div class="yc_title current">
                        <a href="/Customer/Index" class="yc_tab last_tab"><i>客户服务</i></a>
                    </div>
                </div>
                <div class="yc_clear">.</div>
            </div>
        </div>
    </div>

    <div style="margin-top:10px">
        @*@RenderSection("featured", required: false)*@
        @RenderBody()
    </div>
    <footer>
            <div class="">Copyright© 2015 hfsshy.com All Rights reserverd   合肥市书画院   地址：合肥市淮河路与阜阳路交口北20米</div>
            <div class="">建议使用1024*768以上的屏幕分辨率和6.0以上版本的IE来访问本站</div>
    </footer>
    @Scripts.Render("~/Scripts/MySlide.js")
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
